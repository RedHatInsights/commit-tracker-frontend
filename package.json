{
  "name": "commit-tracker",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true npm run start",
    "start:proxy": "PROXY=true npm run start",
    "start:proxy:beta": "BETA=true npm run start:proxy",
    "test": "jest --verbose",
    "verify": "npm-run-all build lint test"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)"
  ],
  "dependencies": {
    "@babel/runtime": "7.15.3",
    "@patternfly/patternfly": "4.132.2",
    "@patternfly/react-core": "4.152.4",
    "@patternfly/react-table": "4.29.58",
    "@redhat-cloud-services/frontend-components": "^3.3.18",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.4",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.1",
    "redux": "4.1.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-flow": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@redhat-cloud-services/frontend-components-config": "4.3.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.1.0",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "3.3.4",
    "css-loader": "^6.2.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.1.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-scss": "3.20.1"
  },
  "insights": {
    "appname": "commit-tracker"
  }
}
